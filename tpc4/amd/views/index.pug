extends layout
block content
    .w3-container
        h1 Arquivo de Música Digital
        table.w3-table-all
            tr
                th Título
                th ID
                th Nº Partituras
            each o in obras
              tr
                  td
                    a(href="/"+o.tit)= o.tit
                  td=o.id
                  td=o.nparts
    footer.w3-container.w3-blue
      address Tiago Baptista
